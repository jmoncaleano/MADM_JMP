library(printr)
library(igraph)
library(tidyverse)
Data=read.table("euroroad.txt", header = FALSE, as.is = T)
road=graph_from_data_frame(d=Data, directed=FALSE)
orden <- gorder(road) # Orden: magnitud de V (cantidad de nodos)
size <- gsize(road) # Size: magnitud de E (cantidad de aristas)
densidad <- edge_density(road) # Calcula el coeficiente de densidad del grafo
No_comp <- components(road)$no #cantidad de cluster o componentes conectados del grafo
Size_comp <- sort(components(road)$csize, decreasing = TRUE)[1] #componente mas largo
road_cg <- induced_subgraph(road,vids = groups(components(road))[[2]])
avg_deg <- mean(degree(road_cg)) # Promedio de los grados del grafo
avg_dis <- average.path.length(road_cg) # Promedio de las distancias
diametro <- diameter(road_cg) # Diametro (propiedad global)
coef_clust <- transitivity(road_cg, type="average") # Coeficiente del cluster
